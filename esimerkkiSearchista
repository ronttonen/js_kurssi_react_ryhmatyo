class App extends Component {
  constructor() {
    super();
    this.state = {queryKeyword : '',
      searchableItems : testObject //tähän pitää vaihtaa tyhjä ja mountissa laittaa oikea data
    };
    this.results = {};
    this.formattedResult = [];
  }
  //haetaan data serveri apin avulla kun sivu aukeaa
  //serverissä pitää vaika db:n avulla luoda aina json tiedosto, joka tarjotaan sitten api polun kautta
  //ei mitään hajua toimiiko tä, mutta ainakin perusidea siinä, niinkun aina. Eli componentDidMount() tapahtuu kun sivu
  //avataan, axios muuttaa jsonin automaattisesti objectiksi, ja se tallennetaan stateen, josta react sitä sitten voi
  //manipuloida.
  componenDidMount() {
    axios.get('SERVERIAPINOSOITETÄHÄN.json').then(res => {
      const lounasData = res;
      this.state.searchableItems = lounasData;
    });
  }
  
  //paskaa algoritmii
  handleQuery = (e) => {
    this.results = {};
    this.setState({queryKeyword : e.target.value});
    console.log(this.state.queryKeyword);
    console.log(e.target.value);
    this.searchItems(e.target.value);
  }
  searchItems = (e) => {
    console.log(this.results);
    let object = this.state.searchableItems;
    for (let ravintola in object) {
      for (let paiva in object[ravintola]) {
      if (object[ravintola][paiva].includes(e) && e !== '' && e.length > 3) {
        this.results[ravintola] = object[ravintola][paiva];
      }
      }
    }
    this.formatResult(this.results);
  }
 formatResult = (e) => {
   let resultHTML = [];
   for (let ravintola in e) {
     resultHTML.push(<li className="ravintola-match" key={ravintola}>Löytyy ravintolasta: {ravintola}</li>);
   }
   this.formattedResult = resultHTML;
 }
 
  render() {
    return (
      <div className="App">
      <script src="https://cdnjs.cloudflare.com/ajax/libs/react/15.1.0/react.js"></script>
      <script src="https://cdnjs.cloudflare.com/ajax/libs/react/15.1.0/react-dom.js"></script>
        <input type="text" value={this.state.queryKeyword} onChange={this.handleQuery}/>
        <ul>{this.formattedResult}</ul>
        <p>{this.state.queryKeyword}</p>
      </div>
    );
  }
}

export default App;
